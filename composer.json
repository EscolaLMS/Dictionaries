{
    "name": "escolalms/dictionaries",
    "description": "Escola Headless LMS dictionaries package",
    "type": "package",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "escolalms/auth": "^0",
        "escolalms/core": "^1",
        "laravel/framework": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": ">=5.0",
        "nunomaduro/larastan": "^1.0"
    },
    "authors": [
        {
            "name": "Maciej Rymarz",
            "email": "maciej.rymarz@escolasoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscolaLms\\Dictionaries\\": "src",
            "EscolaLms\\Dictionaries\\Tests\\": "tests",
            "EscolaLms\\Dictionaries\\Database\\Factories\\": "database/factories",
            "EscolaLms\\Dictionaries\\Database\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscolaLms\\Dictionaries\\EscolaLmsDictionariesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
